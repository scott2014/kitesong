{
    "blocks": {
        "Holo": {},
        "HoloBar": {},
        "HoloBarDark | HoloBar": {
            "android:actionBarWidgetTheme": "@style/Holo.Theme"
        },
        "HoloBarLight | HoloBar": {
            "android:actionBarWidgetTheme": "@style/Holo.Theme.Light"
        },
        "HoloDark | Holo": {},
        "HoloLight | Holo": {}
    },
    "data": {
        "Holo.ListView.Base < @android:Widget.Holo.ListView": {},
        "Holo.Theme | HoloDark HoloBarDark < Holo.Base.Theme": {
            "holoTheme": "dark"
        },
        "Holo.Theme.Light | HoloLight HoloBarLight < Holo.Base.Theme.Light": {
            "holoTheme": "light"
        },
        "Holo.Theme.Light.DarkActionBar | HoloLight HoloBarDark < Holo.Base.Theme.Light.DarkActionBar": {
            "holoTheme": "mixed"
        }
    },
    "include": [
        {
            "name": "themes-core.json:styles",
            "type": "only blocks"
        }
    ],
    "output": "values-v11/themes.xml"
}
